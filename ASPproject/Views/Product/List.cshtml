


@model ProductListViewModel

@foreach(var p in Model.Products)
{
    @Html.Partial("ProductSummary", p)
}
<!--PageLinkHelper modifies this tag according to PagingInfo-->
<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class="btn" page-class-normal="btn-secondary"
     page-class-selected="btn-primary" class="btn-group m-1"></div>